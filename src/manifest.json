{
  "manifest_version": 3,
  "name": "qTest Manager Assistant",
  "version": "1.0",
  "description": "Adds extra functionality to make everyday work with qTest Manager easier.",
  "icons": {
    "48": "icons/puzzle48.png",
    "128": "icons/puzzle128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.qtestnet.com/*"],
      "js": ["bundle.js"]
    }
  ],
  "host_permissions": ["*://*.qtestnet.com/*"]
}
