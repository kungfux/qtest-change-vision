{
  "manifest_version": 3,
  "name": "qTest Manager Assistant",
  "version": "0.1",
  "description": "Add extra functionality to qTest Manager to make the work easier.",
  "icons": {
    "48": "icons/puzzle48.png",
    "128": "icons/puzzle128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.qtestnet.com/*"
      ],
      "js": [
        "bundle.js"
      ]
    }
  ],
  "host_permissions": [
    "*://*.qtestnet.com/*"
  ]
}